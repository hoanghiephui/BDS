<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:state_selected="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <stroke android:width="1.0px" android:color="#ff757575" />
            <solid android:color="#ffbbbbbb" />
        </shape>
    </item>
    <item android:state_enabled="false" android:state_selected="false">
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <stroke android:width="1.0px" android:color="#ff757575" />
            <solid android:color="#ffeaeaea" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="#ffffa409" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="#ffffa409" />
        </shape>
    </item>
    <item android:state_selected="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="#ffffa409" />
        </shape>
    </item>
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <stroke android:width="1.0dip" android:color="#ffe1e1e1" />
            <solid android:color="#ffffffff" />
        </shape>
    </item>
</selector>