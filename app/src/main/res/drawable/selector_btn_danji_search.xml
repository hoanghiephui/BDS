<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="2.0px" android:color="@color/grey9" />
            <solid android:color="@color/grey9" />
            <corners android:radius="50.0dip" />
        </shape>
    </item>
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <stroke android:width="2.0px" android:color="@color/grey9" />
            <solid android:color="@color/grey9" />
            <corners android:radius="50.0dip" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="2.0px" android:color="@color/white1" />
            <solid android:color="@color/white1" />
            <corners android:radius="50.0dip" />
        </shape>
    </item>
</selector>